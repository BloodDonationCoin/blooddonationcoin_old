<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="MoneroWallet.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>
    <!-- Leave WalletPath empty to look in the directory the MoneroWallet resides. -->
    <add key="DaemonFileName" value="bbrd.exe"/>
    <add key="WalletClientFileName" value="simplewallet.exe"/>
    <add key="MinerFileName" value="simpleminer.exe"/>
    <add key="WalletExtensions" value="wallet;bin"/>
    <add key="MiningPoolAddresses" value="bbr-pool.ddns.net:3333;bbr-pool.ddns.net:5555;bbr-pool.ddns.net:7777"/>
    <add key="WalletRefreshInterval" value="10000"/>
    <add key="DaemonPingInterval" value="1000"/>
    <add key="DaemonConnectionCountInterval" value="5000"/>
  </appSettings>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <userSettings>
    <MoneroWallet.Properties.Settings>
      <setting name="WalletPath" serializeAs="String">
        <value />
      </setting>
    </MoneroWallet.Properties.Settings>
  </userSettings>
</configuration>